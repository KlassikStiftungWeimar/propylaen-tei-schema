            </oneOrMore>
            <optional>
               <ref name="tei_back"/>
            </optional>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="propy-facsimile-no_facsimile_text_nodes-constraint-rule-15">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
