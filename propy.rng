   <define name="tei_zone">
      <element name="zone">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">beschreibt eine beliebige zweidimensionale Fl√§che innerhalb eines <code xmlns="http://www.w3.org/1999/xhtml">&lt;surface&gt;</code>-Elements [ ]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="tei_model.gLike"/>
               <ref name="tei_model.graphicLike"/>
               <ref name="tei_model.global"/>
