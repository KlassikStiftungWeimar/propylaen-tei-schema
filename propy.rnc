         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "propy-damageSpan-damageSpan-requires-spanTo-fr-constraint-assert-21"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
